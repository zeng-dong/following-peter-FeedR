{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "yarp": {
    "routes": {
      "aggregator": {
        "clusteredId": "aggregator",
        "match": {
          "path": "aggregator/{**catch-all}"
        },
        "transforms": [
          {
            "pathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "clusters": {
      "aggregator": {
        "destination": {
          "destination1": {
            "address": "http://localhost:5010"
          }
        }
      }
    }
  }
}